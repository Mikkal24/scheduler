{
  "name": "scheduler",
  "version": "1.0.0",
  "description": "Open source application for scheduling tasks (processes)",
  "main": ".webpack/main",
  "authors": "michael sorensen",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-css": "webpack -w -c webpack.style.config.js",
    "build-main": "cross-env ENVIRONMENT=electron webpack -c webpack.main.js",
    "build-renderer": "webpack -c webpack.config.js",
    "start-electron": "cross-env ENVIRONMENT=electron electron .",
    "start": "electron-forge start",
    "package-app": "electron-packager .",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^5.0.1",
    "electron": "^11.2.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@material/button": "^9.0.0",
    "@material/card": "^9.0.0",
    "@material/fab": "^9.0.0",
    "@material/icon-button": "^9.0.0",
    "@material/linear-progress": "^9.0.0",
    "@material/progress-indicator": "^9.0.0",
    "@material/select": "^9.0.0",
    "@material/textfield": "^9.0.0",
    "@material/theme": "^9.0.0",
    "@material/top-app-bar": "^9.0.0",
    "@svgdotjs/svg.js": "^3.0.16",
    "electron-squirrel-startup": "^1.0.0",
    "open": "^7.4.2",
    "resolve-url-loader": "^3.1.2",
    "update-electron-app": "^2.0.1",
    "winston": "^3.3.3"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "osxSign": {
          "identity": "Developer ID Application: Felix Rieseberg (LT94ZKYDCJ)",
          "hardened-runtime": true,
          "entitlements": "entitlements.plist",
          "entitlements-inherit": "entitlements.plist",
          "signature-flags": "library"
        },
        "osxNotarize": {
          "appleId": "felix@felix.fun",
          "appleIdPassword": "my-apple-id-password"
        }
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "scheduler"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/renderer.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  }
}
